Copy from WinScp or FileZill from Local Machine to Linux Server 
# sudo cp -pv /home/ubuntu/jfrog-artifactory-oss-6.1.0.zip .

or

# wget https://s3.amazonaws.com/kkummari/jfrog-artifactory-oss-6.1.0.zip

# apt install unzip 

# unzip jfrog-artifactory-oss-6.1.0.zip

# mv artifactory-oss-6.1.0/ artifactory

# cd /opt/artifactory/bin

# ./installService.sh

# systemctl start artifactory.service

